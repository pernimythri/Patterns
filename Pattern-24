void pattern24(int n)
{
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            if(i==0 || j==0 || i==n-1 || j==n-1)
            {
                cout<<"*";
            }
            else
            {
            cout<<" ";
        }}
        cout<<endl;
    }
}
int main()
{  
    int t;
    cout<<"enter t ";
    cin>>t;
    for(int i=0;i<t;i++)
    {
    int n;
    cout<<"enter n ";
    cin>>n;
    pattern24(n);
    }
}
