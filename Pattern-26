void pattern26(int n)
{
    int i,k,temp=1;
    for(int i=1;i<=n;i++)
    {
        for(int j=temp;j<=n;j++)
        {
            cout<<j;
        }
        
        temp=temp+1;
        for(int j=1;j<temp-1;j++)
           {
               cout<<j ;
            }
              cout<<endl;
              }
     }
int main()
{  
    int t;
    cout<<"enter t ";
    cin>>t;
    for(int i=0;i<t;i++)
    {
    int n;
    cout<<"enter n ";
    cin>>n;
    pattern26(n);
    }
}
