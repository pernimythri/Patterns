void pattern16(int n)
{
     int num=2;
    for(int i=1;i<=n;i++)
    {
        for(int j=1;j<=i;j++)
        {
            cout<<num;
            cout<<" ";
            num=num+2;
        }
        cout<<endl;
    }
}

int main()
{  
    int t;
    cout<<"enter t ";
    cin>>t;
    for(int i=0;i<t;i++)
    {
    int n;
    cout<<"enter n ";
    cin>>n;
    pattern16(n);
    }
}
